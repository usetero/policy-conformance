{
  "policies": [
    {
      "id": "rename-upsert",
      "name": "Rename temp_id to user_id with upsert",
      "log": {
        "match": [{ "log_field": "body", "regex": "^.*$" }],
        "keep": "all",
        "transform": {
          "rename": [
            { "from_log_attribute": "temp_id", "to": "user_id", "upsert": true }
          ]
        }
      }
    }
  ]
}

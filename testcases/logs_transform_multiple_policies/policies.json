{
  "policies": [
    {
      "id": "add-tag1",
      "name": "Add tag1 attribute",
      "log": {
        "match": [
          { "log_field": "body", "regex": "^.*$" }
        ],
        "keep": "all",
        "transform": {
          "add": [
            { "log_attribute": "tag1", "value": "a" }
          ]
        }
      }
    },
    {
      "id": "add-tag2",
      "name": "Add tag2 attribute",
      "log": {
        "match": [
          { "log_field": "body", "regex": "^.*$" }
        ],
        "keep": "all",
        "transform": {
          "add": [
            { "log_attribute": "tag2", "value": "b" }
          ]
        }
      }
    }
  ]
}

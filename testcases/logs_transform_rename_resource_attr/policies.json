{
  "policies": [
    {
      "id": "rename-resource-attr",
      "name": "Rename env to deployment.environment on resource",
      "log": {
        "match": [{ "log_field": "body", "regex": "^.*$" }],
        "keep": "all",
        "transform": {
          "rename": [{ "from_resource_attribute": "env", "to": "deployment.environment" }]
        }
      }
    }
  ]
}

{
  "resourceMetrics": [
    {
      "scopeMetrics": [
        {
          "metrics": [
            {
              "description": "Total HTTP requests",
              "name": "http.requests.total",
              "sum": {
                "aggregationTemporality": "AGGREGATION_TEMPORALITY_CUMULATIVE",
                "dataPoints": [
                  {}
                ]
              },
              "unit": "1"
            },
            {
              "description": "Request duration histogram",
              "histogram": {
                "aggregationTemporality": "AGGREGATION_TEMPORALITY_CUMULATIVE",
                "dataPoints": [
                  {
                    "bucketCounts": [
                      0,
                      0,
                      0
                    ],
                    "explicitBounds": [
                      10,
                      100
                    ]
                  }
                ]
              },
              "name": "request.duration",
              "unit": "ms"
            }
          ]
        }
      ]
    }
  ]
}

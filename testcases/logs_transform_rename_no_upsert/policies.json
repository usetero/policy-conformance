{
  "policies": [
    {
      "id": "rename-no-upsert",
      "name": "Rename temp_id to user_id without upsert (no-op when target exists)",
      "log": {
        "match": [{ "log_field": "body", "regex": "^.*$" }],
        "keep": "all",
        "transform": {
          "rename": [
            {
              "from_log_attribute": "temp_id",
              "to": "user_id",
              "upsert": false
            }
          ]
        }
      }
    }
  ]
}

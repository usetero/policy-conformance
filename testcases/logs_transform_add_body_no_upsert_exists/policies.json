{
  "policies": [
    {
      "id": "add-body-no-upsert-exists",
      "name": "Add body without upsert when body already exists (no-op)",
      "log": {
        "match": [{ "log_field": "severity_text", "exact": "INFO" }],
        "keep": "all",
        "transform": {
          "add": [{ "log_field": "body", "value": "replaced body" }]
        }
      }
    }
  ]
}

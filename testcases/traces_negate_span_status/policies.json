{
  "policies": [
    {
      "id": "drop-non-error",
      "name": "Drop spans that are NOT errors",
      "trace": {
        "match": [
          { "span_status": "SPAN_STATUS_CODE_ERROR", "exists": true, "negate": true }
        ],
        "keep": {
          "percentage": 0.0
        }
      }
    }
  ]
}

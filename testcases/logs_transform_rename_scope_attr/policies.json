{
  "policies": [
    {
      "id": "rename-scope-attr",
      "name": "Rename lib to library.name on scope",
      "log": {
        "match": [{ "log_field": "body", "regex": "^.*$" }],
        "keep": "all",
        "transform": {
          "rename": [{ "from_scope_attribute": "lib", "to": "library.name" }]
        }
      }
    }
  ]
}

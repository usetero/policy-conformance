{
  "policies": [
    {
      "id": "add-tag-no-upsert",
      "name": "Add tag=hello without upsert on non-existent field",
      "log": {
        "match": [
          { "log_field": "body", "regex": "^.*$" }
        ],
        "keep": "all",
        "transform": {
          "add": [
            { "log_attribute": "tag", "value": "hello", "upsert": false }
          ]
        }
      }
    }
  ]
}

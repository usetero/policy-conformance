{
  "resourceMetrics": [
    {
      "resource": {
        "attributes": [
          {
            "key": "service.name",
            "value": {
              "stringValue": "metrics-svc"
            }
          }
        ]
      },
      "scopeMetrics": [
        {
          "metrics": [
            {
              "description": "Request size histogram",
              "histogram": {
                "aggregationTemporality": "AGGREGATION_TEMPORALITY_CUMULATIVE",
                "dataPoints": [
                  {
                    "attributes": [
                      {
                        "key": "source",
                        "value": {
                          "stringValue": "external"
                        }
                      }
                    ],
                    "bucketCounts": [
                      "5",
                      "15",
                      "20",
                      "10"
                    ],
                    "count": "50",
                    "explicitBounds": [
                      100,
                      1000,
                      10000
                    ],
                    "startTimeUnixNano": "1000000000",
                    "sum": 25000,
                    "timeUnixNano": "2000000000"
                  }
                ]
              },
              "name": "request.size",
              "unit": "bytes"
            },
            {
              "description": "CPU gauge",
              "gauge": {
                "dataPoints": [
                  {
                    "asDouble": 45.2,
                    "attributes": [
                      {
                        "key": "source",
                        "value": {
                          "stringValue": "external"
                        }
                      }
                    ],
                    "startTimeUnixNano": "1000000000",
                    "timeUnixNano": "2000000000"
                  }
                ]
              },
              "name": "cpu.usage",
              "unit": "percent"
            }
          ]
        }
      ]
    }
  ]
}
